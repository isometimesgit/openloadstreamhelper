{
    "manifest_version": 2,
    "name": "Openload stream helper",
    "description": "Converts openload.co download URL requests to directory URL ready for streaming.",
    "version": "1.2",
    "permissions": [
        "https://openload.co/*",
        "webRequest",
        "webRequestBlocking",
        "clipboardWrite"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}